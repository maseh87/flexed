flexbox()
  display -webkit-box
  display -moz-box
  display -ms-flexbox
  display -webkit-flex
  display flex

append(values...)
  -webkit-box-flex values
  -moz-box-flex values
  -webkit-flex values
  -ms-flex values
  flex values

order(num)
  -webkit-box-ordinal-group num
  -moz-box-ordinal-group num
  -ms-flex-order num
  -webkit-order num
  order num

